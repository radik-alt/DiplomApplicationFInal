<?xml version="1.0" encoding="utf-8"?>
<layer-list  xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="16"
            android:useLevel="false">
            <solid android:color="#FFFEFE" />
        </shape>
    </item>
    <item>
        <rotate>
            <shape
                android:shape="ring"
                android:thicknessRatio="16"
                android:useLevel="true">
                <gradient
                    android:endColor="@color/middle_priority"
                    android:startColor="@color/low_priority"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>

</layer-list >